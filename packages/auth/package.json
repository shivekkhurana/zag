{
  "name": "@ai26/zag-auth",
  "version": "0.1.0",
  "description": "ZeroAgentGate authentication verification library",
  "type": "module",
  "main": "./dist/core.js",
  "types": "./dist/core.d.ts",
  "exports": {
    ".": {
      "types": "./dist/core.d.ts",
      "import": "./dist/core.js"
    },
    "./adapters/hono": {
      "types": "./dist/adapters/hono.d.ts",
      "import": "./dist/adapters/hono.js"
    },
    "./storage": {
      "types": "./dist/storage/index.d.ts",
      "import": "./dist/storage/index.js"
    },
    "./storage/drizzle": {
